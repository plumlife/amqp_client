{deps, [
  {rabbit_common, ".*", {git, "git://github.com/jbrisbin/rabbit_common.git", {tag, "rabbitmq-3.4.0-community"}}}
]}.

{erl_opts, [
  debug_info,
  compressed,
  report,
  warn_export_all,
  nowarn_export_vars,
  warn_shadow_vars,
  warn_unused_function,
  warn_deprecated_function,
  warn_obsolete_guard,
  warn_unused_import
]}.
